{
  "name": "typings-npm-prompt",
  "version": "1.0.0",
  "description": "Typescript typings for prompt",
  "author": "Bruno Salesses <https://github.com/sinse>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinse/npm-prompt/issues"
  },
  "homepage": "https://github.com/sinse/npm-prompt",
  "scripts": {
    "lint": "echo linting... && tslint test.ts -e \"node_modules/**\" -e \"source/**\" -e \"typings/**\"",
    "build": "echo building... && typings bundle -o bundle.d.ts",
    "test": "echo testing... && ts-node test.ts",
    "all": "npm run lint && npm run build && npm run test"
  },
  "devDependencies": {
    "prompt": "^1.0.0",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-typings": "^0.2.4",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
